{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://tunca-admin.digitalvoyage.agency/$1",
      "permanent": true
    }
  ]
}
